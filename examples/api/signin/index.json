{
    "$delay": 500,
    "$switch": "username",
    "$case": {
        "john": {
            "$switch": "password",
            "$case": {
                "password": {}
            },
            "$default": {
                "$status": {
                    "code": 403,
                    "text": "Forbidden"
                },
                "$data": {
                    "error": "Invalid password"
                }
            }
        },
        "delay": {
            "$delay": 2000,
            "$status": {
                "code": 403,
                "text": "Forbidden"
            },
            "$data": {
                "error": "Failure with delay"
            }
        },
        "timeout": {
            "$delay": 4000,
            "$timeout": true
        }
    },
    "$default": {
        "$status": {
            "code": 403,
            "text": "Forbidden"
        },
        "$data": {
            "error": "Invalid username"
        }
    }
}
